<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@drawable/bakground"
    tools:context=".fragments.MessegeFragment">

    <ImageView
        android:id="@+id/imageView24"
        android:layout_width="413dp"
        android:layout_height="72dp"
        android:background="#7C7B7B"
        tools:layout_editor_absoluteX="0dp"
        tools:layout_editor_absoluteY="-1dp"
        tools:ignore="DuplicateIds,MissingConstraints" />

    <ImageButton
        android:id="@+id/imageButton04"
        android:layout_width="48dp"
        android:layout_height="41dp"
        android:layout_marginStart="5dp"
        android:layout_marginTop="17dp"
        android:src="@drawable/backnew"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <TextView
        android:id="@+id/textView10"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="52dp"
        android:layout_marginTop="16dp"
        android:text="Inbox"
        android:textColor="@color/white"
        android:textSize="30dp"
        android:textStyle="bold"
        app:layout_constraintStart_toStartOf="@+id/imageView24"
        app:layout_constraintTop_toTopOf="parent"
        tools:ignore="MissingConstraints" />

    <ScrollView
        android:layout_width="409dp"
        android:layout_height="657dp"
        android:layout_marginStart="1dp"
        android:layout_marginTop="1dp"
        android:layout_marginEnd="1dp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.0"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/imageView24">

        <androidx.constraintlayout.widget.ConstraintLayout
            android:layout_width="match_parent"
            android:layout_height="match_parent">

            <EditText
                android:id="@+id/edittext_searchbar"
                android:layout_width="379dp"
                android:layout_height="44dp"
                android:layout_marginTop="16dp"
                android:background="@drawable/rounded_corner"
                android:drawableLeft="@drawable/search"
                android:hint="Search"
                android:textColor="@color/black"
                android:textStyle="bold"
                app:layout_constraintBottom_toTopOf="@+id/imageViewUser3"
                app:layout_constraintEnd_toEndOf="parent"
                app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintTop_toTopOf="parent"
                app:layout_constraintVertical_bias="0.0" />

            <de.hdodenhof.circleimageview.CircleImageView
                android:id="@+id/imageViewUser1"
                android:layout_width="60dp"
                android:layout_height="60dp"
                android:layout_centerHorizontal="true"
                android:src="@drawable/person"
                app:civ_border_color="#FE2C54"
                app:civ_border_width="2dp"
                app:layout_constraintBottom_toTopOf="@+id/gl2"
                app:layout_constraintEnd_toStartOf="@+id/imageViewUser2"
                app:layout_constraintHorizontal_bias="0.5"
                app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintTop_toTopOf="@+id/gl1" />

            <de.hdodenhof.circleimageview.CircleImageView
                android:id="@+id/imageViewUser2"
                android:layout_width="60dp"
                android:layout_height="60dp"
                android:layout_centerHorizontal="true"
                android:src="@drawable/person"
                app:civ_border_color="#FE2C54"
                app:civ_border_width="2dp"
                app:layout_constraintBottom_toTopOf="@+id/gl2"
                app:layout_constraintEnd_toStartOf="@+id/imageViewUser3"
                app:layout_constraintHorizontal_bias="0.5"
                app:layout_constraintStart_toEndOf="@+id/imageViewUser1"
                app:layout_constraintTop_toTopOf="@+id/gl1" />

            <de.hdodenhof.circleimageview.CircleImageView
                android:id="@+id/imageViewUser3"
                android:layout_width="60dp"
                android:layout_height="60dp"
                android:layout_centerHorizontal="true"
                android:src="@drawable/person"
                app:civ_border_color="#FE2C54"
                app:civ_border_width="2dp"
                app:layout_constraintBottom_toTopOf="@+id/gl2"
                app:layout_constraintEnd_toStartOf="@+id/imageViewUser4"
                app:layout_constraintHorizontal_bias="0.5"
                app:layout_constraintStart_toEndOf="@+id/imageViewUser2"
                app:layout_constraintTop_toTopOf="@+id/gl1" />

            <de.hdodenhof.circleimageview.CircleImageView
                android:id="@+id/imageViewUser4"
                android:layout_width="60dp"
                android:layout_height="60dp"
                android:layout_centerHorizontal="true"
                android:src="@drawable/person"
                app:civ_border_color="#FE2C54"
                app:civ_border_width="2dp"
                app:layout_constraintBottom_toTopOf="@+id/gl2"
                app:layout_constraintEnd_toStartOf="@+id/imageViewUser5"
                app:layout_constraintHorizontal_bias="0.5"
                app:layout_constraintStart_toEndOf="@+id/imageViewUser3"
                app:layout_constraintTop_toTopOf="@+id/gl1" />

            <de.hdodenhof.circleimageview.CircleImageView
                android:id="@+id/imageViewUser5"
                android:layout_width="60dp"
                android:layout_height="60dp"
                android:layout_centerHorizontal="true"
                android:src="@drawable/person"
                app:civ_border_color="#FE2C54"
                app:civ_border_width="2dp"
                app:layout_constraintBottom_toTopOf="@+id/gl2"
                app:layout_constraintEnd_toEndOf="parent"
                app:layout_constraintHorizontal_bias="0.5"
                app:layout_constraintStart_toEndOf="@+id/imageViewUser4"
                app:layout_constraintTop_toTopOf="@+id/gl1" />



            <ImageView
                android:id="@+id/statusOnline1"
                android:layout_width="410dp"
                android:layout_height="68dp"
                android:layout_toRightOf="@id/imageViewUser1"
                app:layout_constraintBottom_toTopOf="@+id/gl4"
                app:layout_constraintEnd_toEndOf="parent"
                app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintTop_toTopOf="@+id/gl3" />

            <ImageView
                android:id="@+id/statusOnline2"
                android:layout_width="410dp"
                android:layout_height="68dp"
                android:layout_toRightOf="@id/imageViewUser1"
                app:layout_constraintBottom_toTopOf="@+id/gl5"
                app:layout_constraintEnd_toEndOf="parent"
                app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintTop_toTopOf="@+id/gl4" />

            <ImageView
                android:id="@+id/statusOnline3"
                android:layout_width="410dp"
                android:layout_height="68dp"
                android:layout_toRightOf="@id/imageViewUser1"
                app:layout_constraintBottom_toTopOf="@+id/gl6"
                app:layout_constraintEnd_toEndOf="parent"
                app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintTop_toTopOf="@+id/gl5" />

            <ImageView
                android:id="@+id/statusOnline4"
                android:layout_width="410dp"
                android:layout_height="68dp"
                android:layout_toRightOf="@id/imageViewUser1"
                app:layout_constraintBottom_toTopOf="@+id/gl7"
                app:layout_constraintEnd_toEndOf="parent"
                app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintTop_toTopOf="@+id/gl6" />


            <ImageView
                android:id="@+id/statusOnline5"
                android:layout_width="410dp"
                android:layout_height="68dp"
                android:layout_toRightOf="@id/imageViewUser1"
                app:layout_constraintBottom_toTopOf="@+id/gl8"
                app:layout_constraintEnd_toEndOf="parent"
                app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintTop_toTopOf="@+id/gl7" />

            <de.hdodenhof.circleimageview.CircleImageView
                android:id="@+id/imageViewUser6"
                android:layout_width="60dp"
                android:layout_height="60dp"
                android:layout_centerHorizontal="true"
                android:src="@drawable/person"
                app:civ_border_color="#FE2C54"
                app:civ_border_width="2dp"
                app:layout_constraintBottom_toTopOf="@+id/gl4"
                app:layout_constraintEnd_toStartOf="@+id/gl10"
                app:layout_constraintTop_toTopOf="@+id/gl3" />

            <de.hdodenhof.circleimageview.CircleImageView
                android:id="@+id/imageViewUser7"
                android:layout_width="60dp"
                android:layout_height="60dp"
                android:layout_centerHorizontal="true"
                android:src="@drawable/person"
                app:civ_border_color="#FE2C54"
                app:civ_border_width="2dp"
                app:layout_constraintBottom_toTopOf="@+id/gl5"
                app:layout_constraintEnd_toStartOf="@+id/gl10"
                app:layout_constraintStart_toStartOf="@+id/gl9"
                app:layout_constraintTop_toTopOf="@+id/gl4" />


            <de.hdodenhof.circleimageview.CircleImageView
                android:id="@+id/imageViewUser8"
                android:layout_width="60dp"
                android:layout_height="60dp"
                android:layout_centerHorizontal="true"
                android:src="@drawable/person"
                app:civ_border_color="#FE2C54"
                app:civ_border_width="2dp"
                app:layout_constraintBottom_toTopOf="@+id/gl6"
                app:layout_constraintEnd_toStartOf="@+id/gl10"
                app:layout_constraintStart_toStartOf="@+id/gl9"
                app:layout_constraintTop_toTopOf="@+id/gl5" />

            <de.hdodenhof.circleimageview.CircleImageView
                android:id="@+id/imageViewUser9"
                android:layout_width="60dp"
