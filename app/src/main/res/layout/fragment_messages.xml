                app:layout_constraintTop_toTopOf="@+id/gl7"/>

            <TextView
                android:id="@+id/doctorname"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_below="@id/statusOnline1"
                android:layout_marginStart="100dp"
                android:layout_marginTop="5dp"
                android:fontFamily="@font/nunito_bold"
                android:text="Doctor Name 2"
                android:textColor="@color/black"
                android:textSize="20dp"
                app:layout_constraintEnd_toEndOf="parent"
                app:layout_constraintHorizontal_bias="0.0"
                app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintTop_toTopOf="@+id/statusOnline2" />

            <TextView
                android:id="@+id/doctorname2"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_below="@id/statusOnline2"
                android:layout_marginStart="100dp"
                android:layout_marginTop="5dp"
                android:fontFamily="@font/nunito_bold"
                android:text="Doctor Name 1"
                android:textColor="@color/black"
                android:textSize="20dp"
                app:layout_constraintEnd_toEndOf="parent"
                app:layout_constraintHorizontal_bias="0.0"
                app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintTop_toTopOf="@+id/statusOnline1" />

            <TextView
                android:id="@+id/doctorname3"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_below="@id/statusOnline1"
                android:layout_marginStart="100dp"
                android:layout_marginTop="5dp"
                android:fontFamily="@font/nunito_bold"
                android:text="Doctor Name 3"
                android:textColor="@color/black"
                android:textSize="20dp"
                app:layout_constraintEnd_toEndOf="parent"
                app:layout_constraintHorizontal_bias="0.0"
                app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintTop_toTopOf="@+id/statusOnline3" />

            <TextView
                android:id="@+id/doctorname4"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_below="@id/statusOnline1"
                android:layout_marginStart="100dp"
                android:layout_marginTop="5dp"
                android:fontFamily="@font/nunito_bold"
                android:text="Doctor Name 4"
                android:textColor="@color/black"
                android:textSize="20dp"
                app:layout_constraintEnd_toEndOf="parent"
                app:layout_constraintHorizontal_bias="0.0"
                app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintTop_toTopOf="@+id/statusOnline4" />

            <TextView
                android:id="@+id/doctorname5"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_below="@id/statusOnline1"
